{
  "meta": {},
  "spec": {
    "master_key_vars": [
      "alpha"
    ],
    "common_vars": [
      "b_{l}",
      "b'_{l}",
      "b_{1, att}_[att:ATTR_UNI]"
    ],
    "key_polys": [
      "k_{1, l} : G = alpha + <rgid>*b_{l} + r_{l}*b'_{l}",
      "(k_{2, att} : G = r_{l}*b_{1, att})_[att:USER_ATTRS]"
    ],
    "cipher_polys": [
      "cm : Gt = <secret>",
      "(c_{1, j} : H = <mu>_{j} + s_{1, j}*b_{l})_[j:LSSS_ROWS]",
      "(c_{2, j} : H = s_{1, j}*b'_{l} + s_{2, j.dedup}*b_{1, j.attr})_[j:LSSS_ROWS]",
      "(c'_{j} : Gt = <lambda>_{j} + alpha*s_{1, j})_[j:LSSS_ROWS]"
    ],
    "e_vec": [
      "(c'_{j} = <epsilon>_{j})_[j:LIN_COMB]"
    ],
    "e_mat": [
      "(s_{2, j.dedup} ~ k_{2, j.attr} = -<epsilon>_{j})_[j:LIN_COMB]",
      "(s_{1, j} ~ k_{1, l} = -<epsilon>_{j})_[j:LIN_COMB]",
      "(c_{1, j} ~ <rgid> = <epsilon>_{j})_[j:LIN_COMB]",
      "(c_{2, j} ~ r_{l} = <epsilon>_{j})_[j:LIN_COMB]"
    ],
    "fdh_map": []
  }
}
